<h1>New Post</h1>
  <%= form_for :picture, url: pictures_path do |f| %>
      <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
      </p>

      <p>
        <%= f.label :image%><br>
        <%= f.file_field :image%>
      </p>

      <p>
        <%= f.label :description %><br>
        <%= f.text_area :description %>
      </p>

      <p>
        <%= f.submit %>
      </p>
<% end %>